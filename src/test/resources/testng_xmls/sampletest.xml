<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Quick Test Example" parallel="tests" thread-count="1">
	<listeners>
		<listener class-name="com.orasi.utils.debugging.Screenshot" />
	</listeners>
	<parameter name="browserUnderTest" value="chrome" />
	<parameter name="environment" value="test" />
	<parameter name="runLocation" value="perfecto" />
